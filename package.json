{
  "name": "8000000",
  "version": "2.0.0",
  "description": "Cultural activities tracking app on Starknet - Shrine visit habit formation system",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/web",
    "build": "npm run build --workspace=apps/web",
    "test": "npm run test --workspaces",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "contracts:build": "npm run build --workspace=packages/contracts",
    "contracts:test": "npm run test --workspace=packages/contracts",
    "mobile:build": "npm run build --workspace=apps/mobile",
    "mobile:sync": "npm run sync --workspace=apps/mobile"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "shrine",
    "cultural-activities",
    "habit-formation",
    "starknet",
    "blockchain",
    "react",
    "pwa",
    "mobile"
  ],
  "author": "8000000 Development Team",
  "license": "MIT"
}